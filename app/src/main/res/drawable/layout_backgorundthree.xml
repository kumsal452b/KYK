<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools" tools:ignore="ExtraText">
    <item android:state_pressed="false">
        <shape>
            <corners android:topLeftRadius="25dp" android:bottomRightRadius="25dp" />
            <solid android:color="#00FFFFFF" />
            <stroke android:width="2dp" android:color="#03A9F4" />
            <padding android:left="10dp" android:right="10dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">

        <shape>
            <corners android:topLeftRadius="25dp" android:bottomRightRadius="25dp" />
            <solid android:color="#1A9191" />
            <stroke android:width="2dp" android:color="#225268" />
        </shape>
    </item>
</selector>